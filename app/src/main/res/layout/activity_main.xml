<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="me.rishabhkhanna.lib.MainActivity">

    <me.rishabhkhanna.customtogglebutton.CustomToggleButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        app:radius="10dp"
        app:checkedBorderColor="#000"
        app:checkedColor="#f32"
        app:uncheckedBorderColor="#000"
        android:textOn="toggle button"
        android:textOff="toggle button"
        android:id="@+id/toggle_button"
        ></me.rishabhkhanna.customtogglebutton.CustomToggleButton>

</FrameLayout>
